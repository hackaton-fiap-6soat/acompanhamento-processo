[run]
branch = True

[report]
exclude_lines =
    # Exclui classes ou métodos com @staticmethod e @abstractmethod
    @staticmethod
    @abstractmethod
    # Exclui o código que nunca será alcançado
    raise NotImplementedError
    raise CustomDynamoDBException
    except ClientError as e:
    if __name__ == .__main__.:

[coverage:report]
# Exclui linhas ou arquivos específicos
omit =
    app/test/*